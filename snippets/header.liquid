<header>
  <h1>
    {{ shop.name }}
  </h1>

  <ul>
    <!-- Main menu -->
    {% for link in linklists['main-menu'].links -%}
      <li>
        {{ link.title | link_to: link.url }}
      </li>
    {%- endfor %}

    <!-- Footer menu -->
    {% for link in linklists.footer.links -%}
      {{ link.title | link_to: link.url }}
    {%- endfor %}
  </ul>
</header>
